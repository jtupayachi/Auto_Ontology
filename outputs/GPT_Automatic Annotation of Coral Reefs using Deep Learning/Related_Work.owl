<?xml version="1.0"?>
            <rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
                    xml:base="http://www.example.com/ontology#"
                    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
                    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
                    xmlns:owl="http://www.w3.org/2002/07/owl#">

                <!-- Ontology Declaration -->
                <owl:Ontology rdf:about="http://www.example.com/ontology"/>

            <!-- Classes -->
<owl:Class rdf:about="#ImageAnalysisTools"/>
<owl:Class rdf:about="#UnderwaterEntities"/>
<owl:Class rdf:about="#EnvironmentalConditions"/>

<!-- Subclasses -->
<owl:Class rdf:about="#Classifier">
    <rdfs:subClassOf rdf:resource="#ImageAnalysisTools"/>
</owl:Class>
<owl:Class rdf:about="#UnderwaterVehicles">
    <rdfs:subClassOf rdf:resource="#UnderwaterEntities"/>
</owl:Class>
<owl:Class rdf:about="#UnderwaterFeatures">
    <rdfs:subClassOf rdf:resource="#UnderwaterEntities"/>
</owl:Class>
<owl:Class rdf:about="#ImageTypes">
    <rdfs:subClassOf rdf:resource="#ImageAnalysisTools"/>
</owl:Class>
<owl:Class rdf:about="#ImageProcessingActions">
    <rdfs:subClassOf rdf:resource="#ImageAnalysisTools"/>
</owl:Class>
<owl:Class rdf:about="#ImageLabels">
    <rdfs:subClassOf rdf:resource="#ImageAnalysisTools"/>
</owl:Class>
<owl:Class rdf:about="#UnderwaterConditions">
    <rdfs:subClassOf rdf:resource="#EnvironmentalConditions"/>
</owl:Class>

<!-- Individuals -->
<owl:NamedIndividual rdf:about="#CoralClassifier">
    <rdf:type rdf:resource="#Classifier"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#CATAMI">
    <rdf:type rdf:resource="#Classifier"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#AUV">
    <rdf:type rdf:resource="#UnderwaterVehicles"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Coral">
    <rdf:type rdf:resource="#UnderwaterFeatures"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Reefs">
    <rdf:type rdf:resource="#UnderwaterFeatures"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Benthic">
    <rdf:type rdf:resource="#UnderwaterFeatures"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Mosaics">
    <rdf:type rdf:resource="#ImageTypes"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Images">
    <rdf:type rdf:resource="#ImageTypes"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Annotation">
    <rdf:type rdf:resource="#ImageProcessingActions"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Validation">
    <rdf:type rdf:resource="#ImageProcessingActions"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Classification">
    <rdf:type rdf:resource="#ImageProcessingActions"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Labels">
    <rdf:type rdf:resource="#ImageLabels"/>
</owl:NamedIndividual>
<owl:NamedIndividual rdf:about="#Degradation">
    <rdf:type rdf:resource="#UnderwaterConditions"/>
</owl:NamedIndividual>
        </rdf:RDF>