<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.w3.org/2002/07/owl#"
    xml:base="http://www.example.com/ontology#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#">

    <!-- Ontology Declaration -->
    <owl:Ontology rdf:about="http://www.example.com/ontology" />

    <!-- State Level DataBase -->


    <!-- Classes -->
    <owl:Class rdf:about="#Database" />
    <owl:Class rdf:about="#Information" />
    <owl:Class rdf:about="#Methodology" />
    <owl:Class rdf:about="#Datasets" />
    <owl:Class rdf:about="#Products" />
    <owl:Class rdf:about="#Sources" />
    <owl:Class rdf:about="#Freight" />
    <owl:Class rdf:about="#Flow" />
    <owl:Class rdf:about="#State-Level" />
    <owl:Class rdf:about="#FAF5" />  <!-- Subclasses -->
    <owl:Class rdf:about="#State-Level Database">
        <rdfs:subClassOf rdf:resource="#Database" />
    </owl:Class>
    <owl:Class rdf:about="#Freight Flow Information">
        <rdfs:subClassOf rdf:resource="#Information" />
    </owl:Class>
    <owl:Class rdf:about="#State-Level Datasets">
        <rdfs:subClassOf rdf:resource="#Datasets" />
    </owl:Class>
    <owl:Class rdf:about="#State-Level Products">
        <rdfs:subClassOf rdf:resource="#Products" />
    </owl:Class>
    <owl:Class rdf:about="#Data Sources">
        <rdfs:subClassOf rdf:resource="#Sources" />
    </owl:Class>
    <owl:Class rdf:about="#Freight Flow">
        <rdfs:subClassOf rdf:resource="#Flow" />
    </owl:Class>  <!-- Individuals -->
    <owl:NamedIndividual rdf:about="#The 50 States and the District of Columbia">
        <rdf:type rdf:resource="#State-Level Database" />
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#FAF5">
        <rdf:type rdf:resource="#Freight Flow Information" />
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#FAF5 Methodology">
        <rdf:type rdf:resource="#Methodology" />
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#1997, 2002, 2007, 2012 Datasets">
        <rdf:type rdf:resource="#State-Level Datasets" />
    </owl:NamedIndividual>

</rdf:RDF>