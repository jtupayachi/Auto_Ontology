dlprov:OptimizationAlgorithm_1 rdf:type dlprov:OptimizationAlgorithm ;
    rdfs:label'stochastic gradient descent (SGD), Adam, or RMSprop'.

dlprov:LearningRateSchedule_1 rdf:type dlprov:LearningRateSchedule ;
